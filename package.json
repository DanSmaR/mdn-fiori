{
  "name": "MDN-Fiori",
  "version": "0.1.0",
  "description": "Front-End style guide and pattern library for MDN Web Docs - Built with cupper",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "serve": "hugo server --baseURL http://localhost:1313",
    "clean": "node lib/clean.js",
    "build": "node lib/build.js",
    "update": "node lib/update.js",
    "a11y": "(npm list pa11y-ci || npm install -g pa11y-ci) && pa11y-ci --sitemap http://localhost:1313/sitemap.xml --config lib/.pa11yci"
  },
  "pre-commit": [
    "build"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:schalkneethling/mdn-fiori.git"
  },
  "author": "Mozilla",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/schalkneethling/mdn-fiori/issues"
  },
  "homepage": "https://github.com/schalkneethling/mdn-fiori/#readme",
  "devDependencies": {
    "pre-commit": "^1.2.2",
    "sw-precache": "^5.2.0"
  },
  "dependencies": {
    "npm": "^5.8.0"
  }
}
